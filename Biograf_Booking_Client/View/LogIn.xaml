<Window x:Class="Biograf_Booking_Client.View.LogIn"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Biograf_Booking_Client.View"
        mc:Ignorable="d"
        Title="Login" Height="202.814" Width="188.311">
    <Grid Margin="0,0,2,5">
        <Button x:Name="button" Content="Login" HorizontalAlignment="Left" Margin="56,133,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>
        <TextBox x:Name="tbUsername" HorizontalAlignment="Left" Height="23" Margin="31,39,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <PasswordBox x:Name="tbPassword" PasswordChar="*" HorizontalAlignment="Left" Height="23" Margin="31,96,0,0"  VerticalAlignment="Top" Width="120" />
        <Label x:Name="label_Copy" Content="Password" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="31,78,0,0"/>
        <Label x:Name="label_Copy1" Content="Username" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="31,21,0,0"/>

    </Grid>
</Window>
